{
    "configurations": [
      {
        "name": "Python Debugger: FastAPI",
        "type": "debugpy",
        "request": "launch",
        "module": "uvicorn",
        "envFile": "${workspaceFolder}/.env",
        "args": ["main:app", "--reload"],
        "jinja": true
      },
      {
        "name": "Python Debugger: Current File",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal"
      }
    ]
  }
  